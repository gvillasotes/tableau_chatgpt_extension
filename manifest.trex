<?xml version="1.0" encoding="utf-8"?>
<manifest manifest-version="0.1"
  xmlns="http://www.tableau.com/xml/extension_manifest">

  <dashboard-extension id="com.gvillasotes.chatgpt" extension-version="1.0.0">
    <default-locale>en_US</default-locale>
    <name resource-id="name"/>
    <description>ChatGPT Assistant for Tableau Dashboards</description>

    <author
      name="Gino Villasotes"
      email="you@example.com"
      organization="Personal Project"
      website="https://github.com/gvillasotes"
    />

    <min-api-version>1.0</min-api-version>

    <source-location>
      <url>https://gvillasotes.github.io/tableau_chatgpt_extension/</url>
    </source-location>

    <icon></icon>

  </dashboard-extension>

  <resources>
    <resource id="name">
      <text locale="en_US">ChatGPT Assistant</text>
      <text locale="fr_BE">ChatGPT Assistant</text>
      <text locale="de_DE">ChatGPT Assistant</text>
    </resource>
  </resources>
</manifest>
